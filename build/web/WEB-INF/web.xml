<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Registration</servlet-name>
        <servlet-class>com.jobportal.controller.Registration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.jobportal.controller.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Contact</servlet-name>
        <servlet-class>com.jobportal.controller.Contact</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Seeker</servlet-name>
        <servlet-class>com.jobportal.controller.Seeker</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Provider</servlet-name>
        <servlet-class>com.jobportal.controller.Provider</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditProfile</servlet-name>
        <servlet-class>com.jobportal.controller.EditProfile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangePassword</servlet-name>
        <servlet-class>com.jobportal.controller.ChangePassword</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateJob</servlet-name>
        <servlet-class>com.jobportal.controller.CreateJob</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CompleteMark</servlet-name>
        <servlet-class>com.jobportal.controller.CompleteMark</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Registration</servlet-name>
        <url-pattern>/Registration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Contact</servlet-name>
        <url-pattern>/Contact</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Seeker</servlet-name>
        <url-pattern>/Seeker</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Provider</servlet-name>
        <url-pattern>/Provider</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditProfile</servlet-name>
        <url-pattern>/EditProfile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangePassword</servlet-name>
        <url-pattern>/ChangePassword</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateJob</servlet-name>
        <url-pattern>/CreateJob</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CompleteMark</servlet-name>
        <url-pattern>/CompleteMark</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
